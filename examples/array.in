a = [1,2,3,4,5]; 
// pass an array to a function is by reference
fun sum(a) {
    var n = len(a);
    var s = 0;
    for (var i=0; i<n; i=i+1) {
        s = s + a[i];
    }
    return s;
}

printf("sum of an array is %d\n", sum(a)); // expects sum of an array is 15
