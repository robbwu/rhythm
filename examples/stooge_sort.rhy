fun swap(A, i, j) {
    var t = A[i];
    A[i] = A[j];
    A[j] = t;
}
// sorts A[i:j] in-place
fun StoogeSort(A, i, j) {
    var n = j - i; 
    if (j-i <= 1) return; 
    if (j-i == 2) {
        if (A[i] > A[i+1]) swap(A, i, i+1); // swap A[i]<->A[j]
        return; 
    }
        
    var m = i + ceil(n*2/3);
    StoogeSort(A, i, m);
    StoogeSort(A, i+floor(n/3), j);
    StoogeSort(A, i, m); 
}

var A = [2, 3, 1, -1, -2, 9, 8, -4, 10];
StoogeSort(A, 0, len(A));
print A; 
